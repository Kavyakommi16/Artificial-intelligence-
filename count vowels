def count_vowels(s):
    # Define the set of vowels
    vowels = "aeiouAEIOU"
    count = 0
    
    # Iterate through each character in the string
    for char in s:
        if char in vowels:
            count += 1
            
    return count

# Example usage
input_string = "Hello World"
vowel_count = count_vowels(input_string)
print(f"The number of vowels in '{input_string}' is: {vowel_count}")
